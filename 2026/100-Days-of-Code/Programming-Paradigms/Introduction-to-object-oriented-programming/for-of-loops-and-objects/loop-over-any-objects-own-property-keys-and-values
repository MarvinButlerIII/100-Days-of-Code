// loop over object's propery keys and valus

var clothingItem = {
    price: 50,
    color: 'beige',
    material: 'cotton',
    season: 'autumn'
}

for( const key of Object.keys(clothingItem) ) {
    console.log(key, ":", clothingItem[key]) // price : 50 color : beige material : cotton season : autumn
}